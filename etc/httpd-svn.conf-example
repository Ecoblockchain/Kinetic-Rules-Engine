DavLockDB "/web/var/DavLock"

<Location /krl>

  DAV svn

  SVNPath /web/svn/krl/

  AuthType Basic

  AuthName "KRL repository"

  AuthUserFile /web/svn/krl/conf/svn-auth-file

  Require valid-user

</Location>
